{
  "name": "@nebulr-group/nblocks-react",
  "version": "3.0.0-beta.2",
  "description": "A slim react library from Nblocks",
  "main": "dist/index.js",
  "module": "nblocks-react",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "Readme.md"
  ],
  "scripts": {
    "build": "rm -rf ./dist && rollup -c --bundleConfigAsCjs",
    "npm-publish": "npm run build && npm publish --access public",
    "npm-publish-beta": "npm run build && npm publish --access public --tag beta",
    "npm-publish-local": "npm run build && npm publish --registry http://verdaccio:4873 --tag alpha",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@nebulr-group/nblocks-ts-client": "^5.0.0 || ^5.0.0-beta.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@nebulr-group/nblocks-ts-client": "^5.0.0-alpha.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.75",
    "rollup": "^4.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4"
  }
}
